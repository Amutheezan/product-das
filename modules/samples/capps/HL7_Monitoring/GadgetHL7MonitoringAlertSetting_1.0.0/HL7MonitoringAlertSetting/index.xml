<?xml version="1.0" encoding="UTF-8" ?>
<Module>
    <ModulePrefs title="Subscriber" description="Subscriber">
        <Require feature="pubsub-2"/>
        <Require feature="dynamic-height"/>
    </ModulePrefs>
    <Content type="html">
        <![CDATA[
        <head>

          <!-- Theme -->
          <link rel="stylesheet" type="text/css" href="/portal/libs/bootstrap_3.3.5/css/bootstrap.min.css" />
          <link rel="stylesheet" type="text/css" href="/portal/libs/font-wso2_1.0.0/css/font-wso2.min.css" />
          <link rel="stylesheet" type="text/css" href="/portal/libs/theme-wso2_1.0/css/theme-wso2.css" />
          <link rel="stylesheet" type="text/css" href="../HL7MonitoringGadgetCommon/css/nanoscroller.css" />
          <link rel="stylesheet" type="text/css" href="../HL7MonitoringGadgetCommon/css/gadget.css" />

          <!-- jQuery -->
          <script src="../HL7MonitoringGadgetCommon/js/jquery-1.10.2.js"></script>
          <script src="../HL7MonitoringGadgetCommon/js/jquery.nanoscroller.min.js"></script>
          <script src="../HL7MonitoringGadgetCommon/js/moment.min.js"></script>
          <script src="js/main.js"></script>

        </head>
        <body>
  <div class="form-group col-xs-3">
    <label for="diseaseAlert">Disease Alert Limit:</label>
    <input type="text" class="form-control input-sm" id="diseaseAlert" value="1000">
  </div>
  <div class="form-group col-xs-3">
    <label for="diseaseEmail">Email Addresss</label>
    <input type="text" class="form-control input-sm" id="diseaseEmail" placeholder="sample@email.com">
  </div>
  <div class="form-group col-xs-4">
    <label for="diseasePhoneNo">Phone Number(with Country Code)</label>
    <input type="text" class="form-control input-sm" id="diseasePhoneNo" placeholder="+9477777777">
  </div>
  <div class="form-group col-xs-2 add-padding-2x">
     <button id="diseaseupdate" class="btn btn-primary btn-lg">Update</button>
   </div>
   <div class="form-group col-xs-3">
     <label for="waitTimeAlert">Wait Time Alert Limit:</label>
     <input type="text" class="form-control input-sm" id="waitTimeAlert" value="1000">
   </div>
   <div class="form-group col-xs-3">
     <label for="waitTimeEmail">Email Addresss</label>
     <input type="text" class="form-control input-sm" id="waitTimeEmail" placeholder="sample@email.com">
   </div>
   <div class="form-group col-xs-4">
     <label for="waitTimePhoneNo">Phone Number(with Country Code)</label>
     <input type="text" class="form-control input-sm" id="waitTimePhoneNo" placeholder="+9477777777">
   </div>
   <div class="form-group col-xs-2 add-padding-2x">
      <button id="waitTimeupdate" class="btn btn-primary btn-lg">Update</button>
    </div>


        </body>
        ]]>
    </Content>
</Module>
